{
  "name": "powderhouse",
  "version": "0.0.1",
  "description": "powderhouse.org front and backend",
  "main": "index.js",
  "scripts": {
    "build": "cd powderhouse.org && yarn install --check-files && cd ../api.powderhouse.org/ && yarn install --check-files && cd .. && yarn install --check-files && npm run backend-build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend-dev": "cd powderhouse.org && npm run dev",
    "backend-build": "cd api.powderhouse.org && npm run build",
    "backend-dev": "cd api.powderhouse.org && npm run develop",
    "dev": "concurrently --kill-others -t --names=\"BACKEND,FRONTEND\" --prefix-colors=\"bgGrey,bgWhite\" \"npm run backend-dev\" \"npm run frontend-dev\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powderhouse/powderhouse.github.io.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/powderhouse/powderhouse.github.io/issues"
  },
  "homepage": "https://github.com/powderhouse/powderhouse.github.io#readme",
  "devDependencies": {
    "concurrently": "^6.4.0",
    "install-peers": "^1.0.3"
  }
}

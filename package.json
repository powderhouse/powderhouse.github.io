{
  "name": "powderhouse",
  "version": "0.0.1",
  "description": "powderhouse.org front and backend",
  "main": "index.js",
  "scripts": {
    "build": "cd powderhouse.org && yarn install && cd ../api.powderhouse.org/ && yarn install && cd .. && yarn install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend-dev": "git secret cat .secrets >> powderhouse.org/.env.local && cd powderhouse.org && npm run dev",
    "backend-dev": "cd api.powderhouse.org && npm run develop",
    "dev": "concurrently \"npm run frontend-dev\" \"npm run backend-dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powderhouse/powderhouse.github.io.git"
  },

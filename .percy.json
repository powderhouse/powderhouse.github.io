{
  "version": 2,
  "snapshot": {
    "widths": [375, 1280],
    "minHeight": 1024,
    "percyCSS": ""
  },
  "discovery": {
    "networkIdleTimeout": 750,
    "disableCache": true,
    "launchOptions": {
      "args": ["--disable-system-font-check"]
    }
  },
  "static": {
    "cleanUrls": false
  },
  "upload": {
    "files": "**/*.{png,jpg,jpeg}",
    "ignore": "",
    "stripExtensions": false
  }
}
